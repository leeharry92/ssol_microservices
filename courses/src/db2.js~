var mongoose = require( 'mongoose' );
var Schema   = mongoose.Schema;

var test_model2 = new Schema({
    user_id    : String,
	name	   : String,
    content    : [String],
    updated_at : Date
});

mongoose.model( 'test_model2', test_model2 ); // model
mongoose.connect( 'mongodb://localhost/server-name2' );

module.exports = exports = mongoose;
